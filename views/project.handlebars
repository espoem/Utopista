<div class="container">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="mr-1" href="https://github.com/{{project}}" target="_blank" alt="Open in GitHub"><i class="fab fa-github-square fa-2x"></i></a>
        <a class="navbar-brand" href="#">{{project}}'s contributions</a>
        <span class="badge badge-primary">{{total}}</span>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="/project/{{project}}/reviewed">Reviewed <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/project/{{project}}/flagged">Flagged</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/project/{{project}}/pending">Pending</a>
                </li>
            </ul>
        </div>
    </nav>

    {{>cards}}
    {{>pagination}}
</div>
